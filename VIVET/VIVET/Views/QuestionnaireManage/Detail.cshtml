
@{
    ViewBag.Title = "Chi Tiết Dữ Liệu";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var datenow = DataConverter.DateTime2UI_Lite(Model.CollectedDate);

}
@using Ehr.Common.Constraint
@using Ehr.Models
@using Ehr.Common.UI;
@model Questionnaire
<style type="text/css">
    .col-md-4 {
        text-align: right;
        padding-top: 5px;
    }

    .card-tille-cus {
        font-weight: 600;
        font-size: large
    }

    b {
        color: red;
    }

    .space {
        margin-left: 15px
    }

    .h5-header {
        font-weight: 900;
        color: #ff6a00;
    }

    .fixed_button {
        position: fixed;
        top: 250px;
        width: 50px;
        height: 50px; /*height: auto;*/
        right: 30px;
        border-radius: 10px; /* Rounded corners */
        border: 2px solid #bababa;
        color: #bababa;
        z-index: 99;
        text-align: center;
        line-height: 1.40em;
        background-color: transparent;
        font-size: 20px; /* Increase font size */
    }

    #header-a {
        color: #bababa;
        margin-bottom: 5px
    }

    #row-header {
        height: 30px;
        width: 30px;
    }

    #ddmnr {
        width: 300px;
        margin-top: 10px;
        margin-right: -10px;
        height: 350px;
        border: 1px solid #bababa;
        border-radius: 10px
    }

    .dropdown-item:hover {
        background-color: #007bff;
        color: #ffffff;
    }

    #closealls:hover {
        background-color: #007bff;
        color: #ffffff;
    }
</style>

<style type="text/css">

    .fixed_button_review {
        position: fixed;
        top: 300px;
        width: 80px;
        height: 45px; /*height: auto;*/
        right: 30px;
        border-radius: 10px; /* Rounded corners */
        z-index: 99;
        text-align: center;
        line-height: 1.40em;
        background-color: transparent;
        font-size: 20px; /* Increase font size */
    }

    .btn-review-custom {
        width: 50px;
        height: 50px;
        font-size: large;
        text-align: center;
        margin-top: 5px;
    }

    #approvePanel, #rejectPanel, #restorePanel {
        margin-top: 10%;
        transform: scale(0.9)
    }
</style>
<div class="content-wrapper">
    <div class="content-header">
        <div class="card card-default color-palette-box">
            <div class="row card-header" style="border:none;">
                <div class="col-sm-6">
                    <h5>
                        Chi tiết dữ liệu
                    </h5>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="~/">Trang chủ</a></li>
                        <li class="breadcrumb-item active"><a href="/Questionnaire/Index">Danh mục dữ liệu</a></li>
                        <li class="breadcrumb-item active">Chi tiết dữ liệu</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
    <section class="content">
        <div class="card">
            <div class="card-body">
                <div class="row">
                    <div class="col-md-1">
                    </div>
                    <div class="col-md-10">
                        <div class="card card-primary">
                            <form id="form-create-questionnaire">
                                <div class="fixed_button_review">
                                    <a href="#" onclick="window.history.back();" class="btn btn-lg btn-review-custom btn-warning float-right"><i class="fa fa-arrow-left"></i></a>
                                </div>
                                <ul class="fixed_button navbar-nav ml-auto">
                                    <li class="nav-link dropdown show">
                                        <a data-toggle="dropdown" href="#" id="header-a">
                                            <span>
                                                <i class="fas fa-angle-double-left" id="row-header"></i>
                                            </span>
                                        </a>
                                        <div class="dropdown-menu  dropdown-menu-right table-responsive p-0" id="ddmnr">
                                            <a id="closealls" class="dropdown-item">
                                                &nbsp; Đóng/Mở tât cả thẻ
                                            </a>
                                            <a href="#DivA" class="dropdown-item">
                                                A. Thông tin chung
                                            </a>
                                            <a href="#DivB" class="dropdown-item">
                                                B. Kháng sinh
                                            </a>
                                            <a href="#DivC" class="dropdown-item">
                                                C. Heo
                                            </a>
                                            <a href="#DivD" class="dropdown-item">
                                                D. Thú nhai lại
                                            </a>
                                            <a href="#DivE" class="dropdown-item">
                                                E. Gia cầm
                                            </a>
                                            <a href="#DivF" class="dropdown-item">
                                                F. Thông tin thêm
                                            </a>
                                            <a href="#DivG" class="dropdown-item">
                                                G. Heo con
                                            </a>
                                            <a href="#DivH" class="dropdown-item">
                                                H. Trâu
                                            </a>
                                            <a href="#DivI" class="dropdown-item">
                                                I. Gia súc
                                            </a>
                                            <a href="#DivJ" class="dropdown-item">
                                                J. Dê
                                            </a>
                                            <a href="#DivK" class="dropdown-item">
                                                K. Cừu
                                            </a>
                                            <a href="#DivL" class="dropdown-item">
                                                L. Ngựa
                                            </a>
                                            <a href="#DivM" class="dropdown-item">
                                                M. Gà
                                            </a>
                                            <a href="#DivN" class="dropdown-item">
                                                N. Vịt
                                            </a>
                                            <a href="#DivO" class="dropdown-item">
                                                O. Vịt xiêm
                                            </a>
                                            <a href="#DivP" class="dropdown-item">
                                                P. Cút
                                            </a>
                                            <a href="#DivQ" class="dropdown-item">
                                                Q. Ngỗng
                                            </a>
                                            <a href="#DivR" class="dropdown-item">
                                                R. Chó
                                            </a>
                                            <a href="#DivS" class="dropdown-item">
                                                S. Mèo
                                            </a>
                                            <a href="#DivT" class="dropdown-item">
                                                T. Bê
                                            </a>
                                            <a href="#DivU" class="dropdown-item">
                                                U. Gà con
                                            </a>

                                        </div>
                                    </li>
                                </ul>

                                @*-----------A-----------*@
                                <div class="card">
                                    <div class="card-header bg-blue" id="DivA">
                                        <h2 class="card-title card-tille-cus">A. Thông tin chung về sản phẩm</h2>
                                        <div class="card-tools">
                                            <button type="button" class="btn btn-tool" id="btn-close-1" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                                <i class="fas fa-minus"></i>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="form-group">
                                            <label>Ngày thu thập thông tin </label>
                                            <div class="input-group-prepend" title="Nhấn vào icon để xóa">
                                                <span class="input-group-text" onclick="clearDate('TimeLine')"><i class="far fa-calendar-alt"></i></span>
                                                <input type="text" name="CollectedDate" value="@datenow" class="form-control ez-date-isnull-full" disabled>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>1. A1. Nguôn gốc sản phẩm <b>*</b></label><i>(Chỉ đánh dấu một ô)</i>
                                            <div class="form-group clearfix">
                                                @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListOrigin)
                                                {
                                                    var cl = "";
                                                    var ck = "";
                                                    if (item.Text == Model.A1__Product_origin)
                                                    {
                                                        ck = "checked";
                                                    }
                                                    if (item.Value != "0")
                                                    {
                                                        cl = "space";
                                                    }
                                                    var id = "radio" + item.Value + "_A1";
                                                    <div class="icheck-primary d-inline @cl">
                                                        <input type="radio" id="@id" value="@item.Value" name="A1__Product_origin" @ck disabled>
                                                        <label for="@id">
                                                            @item.Text
                                                        </label>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>2. A2. Mã sản phẩm <b>*</b></label>
                                            <input type="text" class="form-control" name="A2__Product_code" id="A2__Product_code" value="@Model.A2__Product_code" placeholder="VD: NO_Biopharmachemie_001" disabled />
                                        </div>
                                        <div class="form-group">
                                            <label>3. A3. Tên sản phẩm <b>*</b></label><br />
                                            <input type="text" class="form-control" name="A3__Product_name" id="A3__Product_name" value="@Model.A3__Product_name" placeholder="Nhập tên sản phẩm" disabled />
                                        </div>
                                        <div class="form-group">
                                            <label>4. A4. Công ty <b>*</b></label><br />
                                            <input type="text" class="form-control" name="A4__Company" id="A4__Company" value="@Model.A4__Company" placeholder="Nhập tên công ty đăng ký sản phẩm" disabled />
                                        </div>
                                        <div class="form-group">
                                            <label>5. A5. Dạng sản phẩm <b>*</b></label><i>(Chỉ đánh dấu một ô)</i><br />
                                            @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListType)
                                            {
                                                var cl = "";
                                                var ck = "";
                                                if (item.Text == Model.A5__Type_of_product)
                                                {
                                                    ck = "checked";
                                                }
                                                if (item.Value != "0")
                                                {
                                                    cl = "space";
                                                }
                                                var id = "radio" + item.Value + "_A5";
                                                <div class="icheck-primary d-inline @cl">
                                                    <input type="radio" id="@id" value="@item.Value" name="A5__Type_of_product" @ck disabled>
                                                    <label for="@id">
                                                        @item.Text
                                                    </label>
                                                </div>
                                            }
                                        </div>
                                        <div class="form-group">
                                            <label>6. A6. Các thành phần khác trong sản phấm <b>*</b></label><br />
                                            @{
                                                var cka61 = "";
                                                var cka62 = "";
                                                if (Model.A6__Other_subtance_in_product == "Có")
                                                {
                                                    cka61 = "checked";
                                                }
                                                else if (Model.A6__Other_subtance_in_product == "Không")
                                                {
                                                    cka62 = "checked";
                                                }
                                            }
                                            <div class="icheck-primary d-inline">
                                                <input type="radio" id="A6__Other_subtance_in_product1" value="Có" name="A6__Other_subtance_in_product" @cka61 disabled>
                                                <label for="A6__Other_subtance_in_product1">
                                                    Có
                                                </label>
                                            </div>&nbsp;&nbsp;&nbsp;
                                            <div class="icheck-primary d-inline">
                                                <input type="radio" id="A6__Other_subtance_in_product2" value="Có" name="A6__Other_subtance_in_product" @cka62 disabled>
                                                <label for="A6__Other_subtance_in_product2">
                                                    Không
                                                </label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>7. A7. Trọng lượng/thể tích sản phẩm <b>*</b></label>
                                            <input type="text" value="@Model.A7__Volume_of_product" class="form-control" name="A7__Volume_of_product" id="A7__Volume_of_product" placeholder="Nhập khối lượng" disabled />
                                        </div>
                                        <div class="form-group">
                                            <label>
                                                8. A8. Đơn vị của trọng lượng/ thể tích sản phẩm
                                            </label>
                                            <div class="form-group clearfix">
                                                @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListRank)
                                                {
                                                    var cl = "";
                                                    var ck = "";
                                                    var nulls = "";
                                                    if (item.Value == "6")
                                                    {
                                                        nulls = "style=visibility:hidden";
                                                    }
                                                    if (item.Text == Model.A8__Unit_of_volume_of_product)
                                                    {
                                                        ck = "checked";
                                                    }
                                                    if (item.Value != "0")
                                                    {
                                                        cl = "space";
                                                    }
                                                    var id = "radio" + item.Value + "_A8";
                                                    <div class="icheck-primary d-inline @cl" @nulls>
                                                        <input type="radio" id="@id" value="@item.Value" name="A8__Unit_of_volume_of_product" @ck disabled>
                                                        <label for="@id">
                                                            @item.Text
                                                        </label>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>9. A9. Các định dạng khối lượng/ thể tích khác của sản phẩm</label><br />
                                            <textarea class="form-control" rows="3" name="A9__Other_volume_of_product" id="A9__Other_volume_of_product" placeholder="Nhập thông tin" disabled>@Model.A9__Other_volume_of_product
                                        </textarea>
                                        </div>
                                    </div>
                                </div>

                                @*-----------B-----------*@
                                <div class="card">
                                    <div class="card-header bg-blue" id="DivB">
                                        <h2 class="card-title card-tille-cus">
                                            B. Không tin liên quan tới kháng sinh
                                        </h2>
                                        <div class="card-tools">
                                            <button type="button" class="btn btn-tool" id="btn-close-2" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                                <i class="fas fa-minus"></i>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="form-group">
                                            <label>10. B1. Số lượng kháng sinh trong sản phẩm <b>*</b></label>
                                            <div class="form-group">
                                                <div class="form-group clearfix">
                                                    @for (int i = 1; i <= 5; i++)
                                                    {

                                                        var id = "radio" + i + "_B1";
                                                        var ck = "";
                                                        var cl = "";
                                                        if (i != 1)
                                                        {
                                                            cl = "space";
                                                        }
                                                        if (i == Model.B1__Number_of_antimicrobials_in_product)
                                                        {
                                                            ck = "checked";
                                                        }
                                                        <div class="icheck-primary d-inline" @cl>
                                                            <input type="radio" id="@id" value="@i" name="B1__Number_of_antimicrobials_in_product" @ck disabled>
                                                            <label for="@id">@i</label>
                                                        </div>
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>11. B2.1. Tên kháng sinh thứ nhất <b>*</b></label><br />
                                            <input type="text" class="form-control" value="@Model.B2_1__Antimicrobial_1" name="B2_1__Antimicrobial_1" id="B2_1__Antimicrobial_1" placeholder="Nhập tên kháng sinh" disabled />
                                        </div>
                                        <div class="form-group">
                                            <label>12. B2.2.Liều lượng kháng sinh thứ nhất trong sản phẩm</label><br />
                                            <input type="text" class="form-control" value="@Model.B2_2__Strength_of_antimicrobial_1" name="B2_2__Strength_of_antimicrobial_1" id="B2_2__Strength_of_antimicrobial_1" placeholder="Nhập liều lượng kháng sinh" disabled />
                                        </div>
                                        <div class="form-group">
                                            <label>
                                                13. B2.3. Đơn vị của liểu lượng kháng sinh thứ nhất
                                            </label><i> (Chỉ đánh dấu một ô)</i><br />
                                            <div class="form-group clearfix">
                                                @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListRank)
                                                {
                                                    var cl = "";
                                                    var ck = "";
                                                    var nulls = "";
                                                    if (item.Value == "6")
                                                    {
                                                        nulls = "style=visibility:hidden";
                                                    }
                                                    if (item.Text == Model.B2_3__Units_of_antimicrobial_1)
                                                    {
                                                        ck = "checked";
                                                    }
                                                    if (item.Value != "0")
                                                    {
                                                        cl = "space";
                                                    }
                                                    var id = "radio" + item.Value + "_B2_3";
                                                    <div class="icheck-primary d-inline @cl" @nulls>
                                                        <input type="radio" id="@id" value="@item.Value" name="B2_3__Units_of_antimicrobial_1" @ck disabled>
                                                        <label for="@id">
                                                            @item.Text
                                                        </label>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>
                                                14. B2.4. Lượng kháng sinh tỷ lệ với lượng sản phẩm
                                            </label><br />
                                            <input type="text" class="form-control" value="@Model.B2_4__Per_amount_of_product__antimicrobial_1_" name="B2_4__Per_amount_of_product__antimicrobial_1_" id="B2_4__Per_amount_of_product__antimicrobial_1_" placeholder="Nhập số lượng kháng sinh" disabled />
                                        </div>
                                        <div class="form-group">
                                            <label>
                                                15. B2.5. Đơn vị của kháng sinh thứ nhất (liên kết đến câu hỏi B2.4)
                                            </label><i> (Chỉ đánh dấu một ô)</i><br />
                                            <div class="form-group clearfix">
                                                @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListRank)
                                                {
                                                    var cl = "";
                                                    var ck = "";
                                                    var nulls = "";
                                                    if (item.Value == "6")
                                                    {
                                                        nulls = "style=visibility:hidden";
                                                    }
                                                    if (item.Text == Model.B2_5__Units_of_antimicrobial_1__link_to_question_B2_4_)
                                                    {
                                                        ck = "checked";
                                                    }
                                                    if (item.Value != "0")
                                                    {
                                                        cl = "space";
                                                    }
                                                    var id = "radio" + item.Value + "_B2_5";
                                                    <div class="icheck-primary d-inline @cl" @nulls>
                                                        <input type="radio" id="@id" value="@item.Value" name="B2_5__Units_of_antimicrobial_1__link_to_question_B2_4_" @ck disabled>
                                                        <label for="@id">
                                                            @item.Text
                                                        </label>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>16. B2.6. Lượng sản phẩm tương ứng (liên kết đến câu hỏi B2.4)</label><br />
                                            <input type="text" class="form-control" value="@Model.B2_6__Per_amount_of_product__volume_of_product___link_to_question_B2_4_" name="B2_6__Per_amount_of_product__volume_of_product___link_to_question_B2_4_" id="B2_6__Per_amount_of_product__volume_of_product___link_to_question_B2_4_" placeholder="..." disabled />
                                        </div>
                                        <div class="form-group">
                                            <label>
                                                17. B2.7. Đơn vị của lượng sản phẩm (liên kết đến câu hỏi B2.4)
                                            </label><i> (Chỉ đánh dấu một ô)</i><br />
                                            <div class="form-group clearfix">
                                                @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListRank)
                                                {
                                                    var cl = "";
                                                    var ck = "";
                                                    var nulls = "";
                                                    if (item.Value == "6")
                                                    {
                                                        nulls = "style=visibility:hidden";
                                                    }
                                                    if (item.Text == Model.B2_7__Units_of_product__link_to_question_B2_4_)
                                                    {
                                                        ck = "checked";
                                                    }
                                                    if (item.Value != "0")
                                                    {
                                                        cl = "space";
                                                    }
                                                    var id = "radio" + item.Value + "_B2_7";
                                                    <div class="icheck-primary d-inline @cl" @nulls>
                                                        <input type="radio" id="@id" value="@item.Value" name="B2_7__Units_of_product__link_to_question_B2_4_" @ck disabled>
                                                        <label for="@id">
                                                            @item.Text
                                                        </label>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                        <hr /><hr />


                                        <div class="form-group">
                                            <label>
                                                18. B3.1. Tên kháng sinh thứ hai <b>*</b>
                                            </label><br />
                                            <input type="text" class="form-control" value="@Model.B3_1__Antimicrobial_2" name="B3_1__Antimicrobial_2" id="B3_1__Antimicrobial_2" placeholder="Nhập tên kháng sinh" disabled />
                                        </div>
                                        <div class="form-group">
                                            <label>19. B3.2. Liều lượng kháng sinh thứ hai trong sản phẩm</label><br />
                                            <input type="text" class="form-control" value="@Model.B3_2__Strength_of_antimicrobial_2" name="B3_2__Strength_of_antimicrobial_2" id="B3_2__Strength_of_antimicrobial_2" placeholder="Nhập liều lượng kháng sinh" disabled />
                                        </div>
                                        <div class="form-group">
                                            <label>
                                                20. B3.3. Đơn vị của liều lượng kháng sinh thứ hai
                                            </label><i> (Chỉ đánh dấu một ô)</i><br />
                                            <div class="form-group clearfix">
                                                @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListRank)
                                                {
                                                    var cl = "";
                                                    var ck = "";
                                                    var nulls = "";
                                                    if (item.Value == "6")
                                                    {
                                                        nulls = "style=visibility:hidden";
                                                    }
                                                    if (item.Text == Model.B3_3__Units_of_antimicrobial_2)
                                                    {
                                                        ck = "checked";
                                                    }
                                                    if (item.Value != "0")
                                                    {
                                                        cl = "space";
                                                    }
                                                    var id = "radio" + item.Value + "_B3_3";
                                                    <div class="icheck-primary d-inline @cl" @nulls>
                                                        <input type="radio" id="@id" value="@item.Value" name="B3_3__Units_of_antimicrobial_2" @ck disabled>
                                                        <label for="@id">
                                                            @item.Text
                                                        </label>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>
                                                21. B3.4. Lượng kháng sinh tỉ lệ với lượng sản phẩm
                                            </label><br />
                                            <input type="text" class="form-control" value="@Model.B3_4__Per_amount_of_product__antimicrobial_2_" disabled name="B3_4__Per_amount_of_product__antimicrobial_2_" id="B3_4__Per_amount_of_product__antimicrobial_2_" placeholder="Nhập số lượng kháng sinh" />
                                        </div>
                                        <div class="form-group">
                                            <label>
                                                22. B3.5. Đơn vị của kháng sinh thứ hai (liên kết đến câu hỏi B3.4)
                                            </label><i> (Chỉ đánh dấu một ô)</i><br />
