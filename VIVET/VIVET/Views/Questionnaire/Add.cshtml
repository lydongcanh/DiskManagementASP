    $(function () {
        $("#form-create-questionnaire").submit(function (e) {
            e.preventDefault();
            var serializedForm = $(this).serializeArray();
            console.log(serializedForm);
            var A1__Product_origin = serializedForm.some(val => val.name == "A1__Product_origin" && val.value !== "");
            var A2__Product_code = serializedForm.some(val => val.name == "A2__Product_code" && val.value !== "");
            var A3__Product_name = serializedForm.some(val => val.name == "A3__Product_name" && val.value !== "");
            var A4__Company = serializedForm.some(val => val.name == "A4__Company" && val.value !== "");
            var A5__Type_of_product = serializedForm.some(val => val.name == "A5__Type_of_product" && val.value !== "");
            var A6__Other_subtance_in_product = serializedForm.some(val => val.name == "A6__Other_subtance_in_product" && val.value !== "");
            var A7__Volume_of_product = serializedForm.some(val => val.name == "A7__Volume_of_product" && val.value !== "");
            var A8__Unit_of_volume_of_product = serializedForm.some(val => val.name == "A8__Unit_of_volume_of_product" && val.value !== "");
            var B1__Number_of_antimicrobials_in_product = serializedForm.some(val => val.name == "B1__Number_of_antimicrobials_in_product" && val.value !== "");
            var B2_1__Antimicrobial_1 = serializedForm.some(val => val.name == "B2_1__Antimicrobial_1" && val.value !== "");
            var B6__Target_species_x = serializedForm.some(val => val.name == "B6__Target_species_x" && val.value !== "");
            var B7__Administration_route = serializedForm.some(val => val.name == "B7__Administration_route" && val.value !== "");
            if (!A1__Product_origin) {
                toastr.warning("Bạn chưa chọn nguồn gốc");
            } else if (!A2__Product_code) {
                toastr.warning("Bạn chưa nhập mã sản phẩm");
            } else if (!A3__Product_name) {
                toastr.warning("Bạn chưa nhập tên sản phẩm");
            } else if (!A4__Company) {
                toastr.warning("Bạn chưa nhập tên công ty");
            } else if (!A5__Type_of_product) {
                toastr.warning("Bạn chưa chọn dạng sản phẩm");
            } else if (!A6__Other_subtance_in_product) {
                toastr.warning("Bạn chưa chọn thành phần khác");
            } else if (!A7__Volume_of_product) {
                toastr.warning("Bạn chưa nhập khối lượng/trọng lượng sản phẩm");
            } else if (!A8__Unit_of_volume_of_product) {
                toastr.warning("Bạn chưa chọn đơn vị sản phẩm");
            } else if (!B1__Number_of_antimicrobials_in_product) {
                toastr.warning("Bạn chưa chọn số kháng sinh trong sản phẩm");
            } else if (!B2_1__Antimicrobial_1) {
                toastr.warning("Bạn chưa nhập tên kháng sinh");
            } else if (!B6__Target_species_x) {
                toastr.warning("Bạn chưa chọn loài vật nuôi");
            } else if (!B7__Administration_route) {
                toastr.warning("Bạn chưa chọn đường cấp thuốc");
            } else if (A1__Product_origin && A2__Product_code && A3__Product_name && A4__Company && A5__Type_of_product && A6__Other_subtance_in_product && A7__Volume_of_product &&
                A8__Unit_of_volume_of_product && B1__Number_of_antimicrobials_in_product && B2_1__Antimicrobial_1 && B6__Target_species_x && B7__Administration_route !== "") {
                $.ajax({
                    type: "POST",
                    dataType: "json",
                    data: serializedForm,
                    url: "/Questionnaire/Add",
                    success: function (item) {
                        setTimeout(function () {
                            if (item.success) {
                                toastr.success(item.message);
                                window.location.href = window.location.origin + "/Questionnaire/Index";
                            } else {
                                toastr.error(item.message);
                            }
                        }, 500);

                    },
                    error: function (err) {
                        setTimeout(function () {
                            toastr.error("Thêm dữ liệu thành công", "Failed");
                        }, 500);
                        console.log(err);
                    }
                });
            }
        });

    });

</script>
