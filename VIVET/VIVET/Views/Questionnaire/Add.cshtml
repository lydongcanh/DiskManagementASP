@{
    /**/
    ViewBag.Title = "Nhập Dữ Liệu";
    var datenow = DataConverter.DateTime2UI_Lite(DateTime.Now);
}
@using Ehr.Common.Constraint
@using Ehr.Models
@using Ehr.Common.UI;
@using System.ComponentModel.DataAnnotations;
@using Ehr.Auth;
<style type="text/css">
    .col-md-4 {
        text-align: right;
        padding-top: 5px;
    }

    .card-tille-cus {
        font-weight: 600;
        font-size: large
    }

    b {
        color: red;
    }

    .space {
        margin-left: 15px
    }

    .h5-header {
        font-weight: 900;
        color: #ff6a00;
    }

    .fixed_button {
        position: fixed;
        top: 200px;
        width: 45px;
        height: 45px; /*height: auto;*/
        right: 30px;
        border-radius: 10px; /* Rounded corners */
        border: 2px solid #bababa;
        color: #bababa;
        z-index: 99;
        text-align: center;
        line-height: 1.40em;
        background-color: transparent;
        font-size: 20px; /* Increase font size */
    }

    #header-a {
        color: #bababa;
        margin-bottom: 5px
    }

    #row-header {
        height: 30px;
        width: 30px;
    }

    #ddmnr {
        width: 300px;
        margin-top: 10px;
        height: 350px;
        border: 1px solid #bababa;
        border-radius: 5px
    }

    .dropdown-item:hover {
        background-color: #007bff;
        color: #ffffff;
    }

    #closealls:hover {
        background-color: #007bff;
        color: #ffffff;
    }

    .select2-container .select2-selection--single {
        height: calc(2.25rem + 2px);
        padding: .75rem 1rem;
        border: 1px solid #ced4da;
        display: block;
    }

    .select2-container {
        width: 100% !important;
    }

    .select2-selection__arrow {
        padding-top: 2rem;
    }
</style>
<div class="content-wrapper">
    <div class="content-header">
        <div class="card card-default color-palette-box">
            <div class="row card-header" style="border:none;">
                <div class="col-sm-6">
                    <h5>
                        Nhập dữ liệu
                    </h5>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="~/">Trang chủ</a></li>
                        <li class="breadcrumb-item active"><a href="/Questionnaire/Index">Danh mục dữ liệu</a></li>
                        <li class="breadcrumb-item active">Nhập dữ liệu</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
    <section class="content">
        <div class="card">
            <div class="card-body">
                <div class="row">
                    <div class="col-md-1">
                    </div>
                    <div class="col-md-10">
                        <div class="card card-primary">
                            <form id="form-create-questionnaire">
                                <ul class="fixed_button navbar-nav ml-auto">
                                    <li class="nav-link dropdown show">
                                        <a data-toggle="dropdown" href="#" id="header-a">
                                            <span>
                                                <i class="fas fa-angle-double-left" id="row-header"></i>
                                            </span>
                                        </a>
                                        <div class="dropdown-menu  dropdown-menu-right table-responsive p-0" id="ddmnr">
                                            <a id="closealls" class="dropdown-item">
                                                &nbsp; Đóng/Mở các thẻ
                                            </a>
                                            <a href="#DivA" class="dropdown-item">
                                                A. Thông tin chung
                                            </a>
                                            <a href="#DivB" class="dropdown-item">
                                                B. Kháng sinh
                                            </a>
                                            <a href="#DivC" class="dropdown-item">
                                                C. Heo
                                            </a>
                                            <a href="#DivD" class="dropdown-item">
                                                D. Thú nhai lại
                                            </a>
                                            <a href="#DivE" class="dropdown-item">
                                                E. Gia cầm
                                            </a>
                                            <a href="#DivF" class="dropdown-item">
                                                F. Thông tin thêm
                                            </a>
                                            <a href="#DivG" class="dropdown-item">
                                                G. Heo con
                                            </a>
                                            <a href="#DivH" class="dropdown-item">
                                                H. Trâu
                                            </a>
                                            <a href="#DivI" class="dropdown-item">
                                                I. Gia súc
                                            </a>
                                            <a href="#DivJ" class="dropdown-item">
                                                J. Dê
                                            </a>
                                            <a href="#DivK" class="dropdown-item">
                                                K. Cừu
                                            </a>
                                            <a href="#DivL" class="dropdown-item">
                                                L. Ngựa
                                            </a>
                                            <a href="#DivM" class="dropdown-item">
                                                M. Gà
                                            </a>
                                            <a href="#DivN" class="dropdown-item">
                                                N. Vịt
                                            </a>
                                            <a href="#DivO" class="dropdown-item">
                                                O. Vịt xiêm
                                            </a>
                                            <a href="#DivP" class="dropdown-item">
                                                P. Cút
                                            </a>
                                            <a href="#DivQ" class="dropdown-item">
                                                Q. Ngỗng
                                            </a>
                                            <a href="#DivR" class="dropdown-item">
                                                R. Chó
                                            </a>
                                            <a href="#DivS" class="dropdown-item">
                                                S. Mèo
                                            </a>
                                            <a href="#DivT" class="dropdown-item">
                                                T. Bê
                                            </a>
                                            <a href="#DivU" class="dropdown-item">
                                                U. Gà con
                                            </a>

                                        </div>
                                    </li>
                                </ul>

                                @*-----------A-----------*@
                                <div class="card">
                                    <div class="card-header bg-blue" id="DivA">
                                        <h2 class="card-title card-tille-cus">A. Thông tin chung về sản phẩm</h2>
                                        <div class="card-tools">
                                            <button type="button" class="btn btn-tool" id="btn-close-1" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                                <i class="fas fa-minus"></i>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="form-group">
                                            <label>Ngày thu thập thông tin </label>
                                            <div class="input-group-prepend" title="Nhấn vào icon để xóa">
                                                <span class="input-group-text" onclick="clearDate('TimeLine')"><i class="far fa-calendar-alt"></i></span>
                                                <input type="text" name="CollectedDate" value="@datenow" class="form-control ez-date-isnull-full" readonly>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>1. A1. Nguôn gốc sản phẩm <b>*</b></label><i>(Chỉ đánh dấu một ô)</i>
                                            <div class="form-group clearfix">
                                                @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListOrigin)
                                                {
                                                    var cl = "";
                                                    if (item.Value != "0")
                                                    {
                                                        cl = "space";
                                                    }
                                                    var id = "radio" + item.Value + "_A1";
                                                    <div class="icheck-primary d-inline @cl">
                                                        <input type="radio" id="@id" value="@item.Text" name="A1__Product_origin">
                                                        <label for="@id">
                                                            @item.Text
                                                        </label>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>2. A2. Mã sản phẩm <b>*</b></label><br />
                                            <i>
                                                Nhập thông tin code sản phẩm đã đăng ký như trong danh sách. Nếu sản phẩm không có code đăng ký thì sử
                                                dụng code sau"NO_tencongty_maso" với NO là ký hiệu chung cho sản phẩm không có số đăng ký, tên công ty
                                                viết đầy đủ, mã số từ 001 tới 100. Ví dụ NO_Biopharmachemie_001.
                                            </i> <br /> <br />
                                            <input type="text" class="form-control" name="A2__Product_code" id="A2__Product_code" placeholder="VD: NO_Biopharmachemie_001" />
                                        </div>
                                        <div class="form-group">
                                            <label>3. A3. Tên sản phẩm <b>*</b></label><br />
                                            <input type="text" class="form-control" name="A3__Product_name" id="A3__Product_name" placeholder="Nhập tên sản phẩm" />
                                        </div>
                                        <div class="form-group">
                                            <label>4. A4. Công ty <b>*</b></label><br />
                                            <input type="text" class="form-control" name="A4__Company" id="A4__Company" placeholder="Nhập tên công ty đăng ký sản phẩm" />
                                        </div>
                                        <div class="form-group">
                                            <label>5. A5. Dạng sản phẩm <b>*</b></label><i>(Chỉ đánh dấu một ô)</i><br />
                                            @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListType)
                                            {
                                                var cl = "";
                                                if (item.Value != "0")
                                                {
                                                    cl = "space";
                                                }
                                                var id = "radio" + item.Value + "_A5";
                                                <div class="icheck-primary d-inline @cl">
                                                    <input type="radio" id="@id" value="@item.Text" name="A5__Type_of_product">
                                                    <label for="@id">
                                                        @item.Text
                                                    </label>
                                                </div>
                                            }
                                        </div>
                                        <div class="form-group">
                                            <label>6. A6. Các thành phần khác trong sản phấm <b>*</b></label><i>(Chỉ đánh dấu một ô)</i><br />
                                            <i>
                                                Chọn "Có" nếu sản phẩm chứa chất khác ngoài kháng sinh. Chọn "Không" nếu sản phẩm chỉ chứa kháng sinh.
                                                Các chất khác bao gồm vitamin, khoáng bổ sung thêm trong sản phẩm. Tá dược không được tính là chất khác
                                            </i><br /><br />
                                            @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListYesNo)
                                            {
                                                var cl = "";
                                                if (item.Value != "0")
                                                {
                                                    cl = "space";
                                                }
                                                var id = "radio" + item.Value + "_A6";
                                                <div class="icheck-primary d-inline @cl">
                                                    <input type="radio" id="@id" value="@item.Text" name="A6__Other_subtance_in_product">
                                                    <label for="@id">
                                                        @item.Text
                                                    </label>
                                                </div>
                                            }
                                        </div>
                                        <div class="form-group">
                                            <label>7. A7. Trọng lượng/thể tích sản phẩm <b>*</b></label><br />
                                            <i>
                                                Điền thông tin khối lượng/trọng lượng/thể tích của sản phẩm tìm thấy, chỉ điền số. Nếu sản phẩm có nhiều
                                                khối lượng thì ưu tiên 1 là điền thông tin khối lượng/trọng lượng/thể tích theo đúng thông tin trên nhãn.
                                                Thông tin này sẽ dùng để tính toán lại lượng kháng sinh trong 1 đơn vị sản phẩm. Trường hợp sản phẩm có
                                                nhiều khối lượng thì bổ sung thông tin về các khối lượng khác trong mục A9.
                                            </i><br /><br />
                                            <input type="text" class="form-control" name="A7__Volume_of_product" id="A7__Volume_of_product" placeholder="Nhập khối lượng" />
                                        </div>
                                        <div class="form-group">
                                            <label>
                                                8. A8. Đơn vị của trọng lượng/ thể tích sản phẩm
                                            </label><i>(Chỉ đánh dấu một ô)</i><br />
                                            <i>
                                                Điền thông tin về đơn vị của khối lượng/trọng lượng/thể tích của sản phẩm tìm thấy
                                            </i> <br /><br />
                                            <div class="form-group clearfix">
                                                @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.ListRank)
                                                {
                                                    var cl = "";
                                                    var check = "";
                                                    var nulls = "";
                                                    if (item.Value == "6")
                                                    {
                                                        check = "checked";
                                                        nulls = "style=visibility:hidden";
                                                    }
                                                    if (item.Value != "0")
                                                    {
                                                        cl = "space";
                                                    }
                                                    var id = "radio" + item.Value + "_A8";
                                                    <div class="icheck-primary d-inline @cl" @nulls>
                                                        <input type="radio" id="@id" value="@item.Text" name="A8__Unit_of_volume_of_product" @check>
                                                        <label for="@id">
                                                            @item.Text
                                                        </label>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>9. A9. Các định dạng khối lượng/ thể tích khác của sản phẩm</label><br />
                                            <i>
                                                Điền thông tin về khối lượng/trọng lượng/thể tích khác tìm thấy. Các số sẽ cách nhau bởi dấu phẩy. Ví dụ
                                                10,30,50. Nếu có nhiều khối lượng với đơn vị khác nhau thì các bạn tự động quy đổi ra đơn vị tương ứng với
                                                đơn vị đã chọn ở ô A8.
                                            </i><br /><br />
                                            <textarea class="form-control" rows="3" name="A9__Other_volume_of_product" id="A9__Other_volume_of_product" placeholder="Nhập các trọng lượng khác của sản phẩm theo đơn vị phía trên ví dụ: 30, 50, 100 ..."></textarea>
                                        </div>
                                    </div>
                                </div>

                                @*-----------B-----------*@
                                <div class="card">
                                    <div class="card-header bg-blue" id="DivB">
                                        <h2 class="card-title card-tille-cus">
                                            B. Không tin liên quan tới kháng sinh
                                        </h2>
                                        <div class="card-tools">
                                            <button type="button" class="btn btn-tool" id="btn-close-2" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                                <i class="fas fa-minus"></i>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="form-group">
                                            <label>10. B1. Số lượng kháng sinh trong sản phẩm <b>*</b></label><i>(Chỉ đánh dấu một ô)</i><br />
                                            <i>
                                                Đánh dấu vào số lượng loại kháng sinh có trong sản phẩm tương ứng
                                            </i><br /><br />
                                            <div class="form-group">
                                                <div class="form-group clearfix">
                                                    <div class="icheck-primary d-inline">
                                                        <input type="radio" id="radio1_B1" value="1" name="B1__Number_of_antimicrobials_in_product">
                                                        <label for="radio1_B1">1</label>
                                                    </div>&nbsp;&nbsp;&nbsp;
                                                    <div class="icheck-primary d-inline">
                                                        <input type="radio" id="radio2_B1" value="2" name="B1__Number_of_antimicrobials_in_product">
                                                        <label for="radio2_B1">2</label>
                                                    </div>&nbsp;&nbsp;&nbsp;
                                                    <div class="icheck-primary d-inline">
                                                        <input type="radio" id="radio3_B1" value="3" name="B1__Number_of_antimicrobials_in_product">
                                                        <label for="radio3_B1">3</label>
                                                    </div>&nbsp;&nbsp;&nbsp;
                                                    <div class="icheck-primary d-inline">
                                                        <input type="radio" id="radio4_B1" value="4" name="B1__Number_of_antimicrobials_in_product">
                                                        <label for="radio4_B1">4</label>
                                                    </div>&nbsp;&nbsp;&nbsp;
                                                    <div class="icheck-primary d-inline">
                                                        <input type="radio" id="radio5_B1" value="5" name="B1__Number_of_antimicrobials_in_product">
                                                        <label for="radio5_B1">5</label>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label>11. B2.1. Tên kháng sinh thứ nhất <b>*</b></label><br />
                                            <select id="B2_1__Antimicrobial_1" class="form-control select2-primary select2" name="B2_1__Antimicrobial_1">
